option_settings:
  aws:elasticbeanstalk:cloudwatch:logs:
    StreamLogs: true
    RetentionInDays: 7
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "stockmanage.settings"
container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python manage.py migrate --noinput"
    leader_only: true